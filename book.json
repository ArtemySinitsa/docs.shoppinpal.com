{
	"gitbook": "3.2.2",
	"plugins": [
		"youtube",
		"theme-gestalt",
		"-theme-default",
		"styles-sass-fix",
		"googledocs",
		"editlink",
		"anchors",
		"ga",
		"rss",
		"mermaid"
	],
	"pluginsConfig": {
		"versions": {
			"type": "tags",
			"includeFilepath": true
		},
		"editlink": {
			"base": "https://github.com/shoppinpal/docs.shoppinpal.com/edit/master",
			"label": "Edit This Page",
			"multilingual": false
		},
		"ga": {
			"token": "UA-96189107-1"
		},
		"googledocs": {
			"rm": "minimal",
			"frameborder": "0",
			"width": "100%",
			"height": "500px",
			"noembed": "new window"
		},
		"rss": {
			"title": "The Ultimate Training Guide",
			"description": "We are a group of startup enthusiasts. This training guide is meant to be the ultimate resource for our internal use and the public eye! We use it to store our cutting-edge research and to train engineers in our startup's technology stack.",
			"author": "ShoppinPal Team",
			"feed_url": "https://training.shoppinpal.com/feed",
			"site_url": "https://training.shoppinpal.com"
		}
	},
	"title": "ShoppinPal Training"
}